RewriteEngine On

# Prevent direct access to msfunction.php
RewriteCond %{THE_REQUEST} ^[A-Z]{3,}\s/+msfunction\.php [NC]
RewriteRule ^ - [F,L]

# Rewrite rule to redirect /register to msfunction.php
RewriteRule ^register$ msfunction.php [L]

<IfModule mod_headers.c>
    Header set X-Frame-Options "DENY"
    Header set X-XSS-Protection "1; mode=block"
    Header set X-Content-Type-Options "nosniff"
    Header set Referrer-Policy "no-referrer"
    Header set Permissions-Policy "geolocation=()"
</IfModule>
